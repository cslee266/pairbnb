<h1>Create New Listing</h1>

  <%= form_for @listing, url: listings_path do |f| %>
	
	  <%= f.label :name %>
  	<%= f.text_field :name %><br>
    
    <%= f.label :property_type %>
  	<%= f.text_field :property_type %><br>

	  <%= f.label :room_number %>
  	<%= f.text_field :room_number %><br>

	  <%= f.label :bed_number %>
  	<%= f.text_field :bed_number %><br>

  	<%= f.label :guest_number %>
  	<%= f.text_field :guest_number %><br>

  	<%= f.label :country %>
  	<%= f.text_field :country %><br>

 	  <%= f.label :state %>
  	<%= f.text_field :state %><br>

  	<%= f.label :city %>
  	<%= f.text_field :city %><br>

  	<%= f.label :zipcode %>
  	<%= f.text_field :zipcode %><br>

  	<%= f.label :address %>
  	<%= f.text_field :address %><br>

	  <%= f.label :price %>
  	<%= f.text_field :price %><br>

  	<%= f.label :description %>
  	<%= f.text_field :description %><br>

  	<%= f.label :photos %>
  	<%= f.file_field :photos, multiple: true %><br>

  	<%= f.submit "Submit listing" %>
  <% end %>