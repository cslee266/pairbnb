<h1>listing#show</h1>

<% user = User.find(@listing.user_id) %>

<h4>Posted by <%= user.name %></h4>

<table>
	<tr>
		<th>Property name</th>
		<th>Property type</th>
		<th>Room Number</th>
		<th>Bed Number</th>
		<th>Guest number</th>
		<th>Country</th>
		<th>State</th>
		<th>City</th>
		<th>Zipcode</th>
		<th>Address</th>
		<th>Price</th>
		<th>Description</th>
	</tr>

<% x = @listing %>
<tr>
<td><%= x.name %></td>
<td><%= x.property_type %></td>
<td><%= x.room_number %></td>
<td><%= x.bed_number %></td>
<td><%= x.guest_number %></td>
<td><%= x.country %></td>
<td><%= x.state %></td>
<td><%= x.city %></td>
<td><%= x.zipcode %></td>
<td><%= x.address %></td>
<td><%= x.price %></td>
<td><%= x.description %></td>
</tr>

</table>